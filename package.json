{
  "name": "handyscript",
  "version": "1.3.0",
  "type": "module",
  "author": "handyscript",
  "description": "A set of useful modules that makes the javascript development context easier",
  "homepage": "https://github.com/handyscript/handyscript/wiki",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handyscript/handyscript.git"
  },
  "license": "MIT",
  "main": "dist/hs.min.cjs",
  "module": "dist/hs.min.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "lib",
    "index.ts",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "npm run lint && tsc test/index.test.ts --noEmit && node test/index.test.cjs",
    "watch": "rollup -c rollup.config.js --config-dev -w",
    "build": "rollup -c rollup.config.js --config-prod"
  },
  "keywords": [
    "package",
    "useful",
    "easier",
    "easy",
    "helpful",
    "help",
    "helpers",
    "es6",
    "es7",
    "js",
    "ts",
    "javascript",
    "typescript",
    "handy-script",
    "handyscript",
    "modules",
    "utils",
    "utilities",
    "utility",
    "newjs",
    "handy",
    "var",
    "let",
    "json",
    "object",
    "array",
    "string",
    "number",
    "boolean",
    "function",
    "class",
    "api",
    "validate",
    "validation",
    "valid",
    "check",
    "checkers",
    "exfiltrate",
    "exfiltration",
    "const"
  ],
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/rollup-win32-x64-msvc": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "rollup": "^4.1.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public",
    "@vvhybe:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "handyutility": "^1.0.1"
  }
}
