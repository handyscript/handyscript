{
  "name": "handyscript",
  "version": "1.9.0-production",
  "type": "module",
  "author": "handyscript",
  "description": "A set of useful modules that makes the javascript development context easier",
  "homepage": "https://handyscript.tech",
  "repository": {
    "type": "git",
    "url": "https://github.com/handyscript/handyscript.git"
  },
  "license": "CC BY-SA 4.0",
  "main": "dist/hs.min.cjs",
  "module": "dist/hs.min.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/hs.min.js",
      "require": "./dist/hs.min.cjs"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "index.ts",
		"AUTHORS.md",
    "README.md",
    "SECURITY.md"
  ],
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint . --ext .ts --fix",
    "test": "npm run lint && jest",
    "build": "webpack",
    "watch": "tsc --project tsconfig.json && webpack --watch",
    "semantic-release": "semantic-release --branches main"
  },
  "keywords": [
    "package",
    "useful",
    "easier",
    "easy",
    "helpful",
    "help",
    "helpers",
    "es6",
    "es7",
    "js",
    "ts",
    "javascript",
    "typescript",
    "handy-script",
    "handyscript",
    "modules",
    "utils",
    "utilities",
    "utility",
    "newjs",
    "handy",
    "var",
    "let",
    "json",
    "object",
    "array",
    "string",
    "number",
    "boolean",
    "function",
    "class",
    "api",
    "validate",
    "validation",
    "valid",
    "check",
    "checkers",
    "exfiltrate",
    "exfiltration",
    "const"
  ],
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^7.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "semantic-release": "^22.0.8",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "publishConfig": {
    "access": "public",
    "@vvhybe:registry": "https://npm.pkg.github.com"
  },
  "dependencies": {
    "axios": "^1.6.1",
    "handyutility": "^1.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
