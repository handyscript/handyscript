{
  "name": "handyscript",
  "version": "1.1.0",
  "type": "module",
  "author": "handyscript",
  "description": "A set of useful modules that makes the javascript development context easier",
  "homepage": "https://github.com/handyscript/handyscript#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handyscript/handyscript.git"
  },
  "license": "MIT",
  "main": "dist/hs.min.cjs",
  "module": "dist/hs.min.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "dist",
    "lib",
    "index.ts",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "test": "tsc test/index.ts && node test/index.js",
    "watch": "rollup -c rollup.config.js --config-dev -w",
    "build": "rollup -c rollup.config.js --config-prod"
  },
  "keywords": [
    "package",
    "useful",
    "easier",
    "easy",
    "helpful",
    "help",
    "helpers",
    "es6",
    "es7",
    "js",
    "ts",
    "javascript",
    "typescript",
    "handy-script",
    "handyscript",
    "modules",
    "utils",
    "utilities",
    "utility",
    "newjs",
    "handy",
    "var",
    "let",
    "json",
    "object",
    "array",
    "string",
    "number",
    "boolean",
    "function",
    "class",
    "api",
    "validate",
    "validation",
    "valid",
    "check",
    "checkers",
    "exfiltrate",
    "exfiltration",
    "const"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "rollup": "^3.27.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.23.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
